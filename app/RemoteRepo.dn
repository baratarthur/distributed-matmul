const int DEFAULT_DIST_PORT = 8081

component provides App requires data.IntUtil iu, server.Remote:matmul service {
    int portDist = 0

    int App:main(AppParam params[]) {
        if (params.arrayLength == 2) {
			portDist = iu.intFromString(params[0].string)
		} else {
			portDist = DEFAULT_DIST_PORT
		} 
		
		service.start(portDist)

        return 0
    }
}